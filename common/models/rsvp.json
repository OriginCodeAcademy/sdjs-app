{
    "name": "rsvp",
    "base": "PersistedModel",
    "idInjection": true,
    "options": {
      "validateUpsert": true
    },
    "properties": {
      "timestamp": {
        "type": "date",
        "default": "$now",
        "required": true
      }
    },
    "validations": [],
    "relations": {
      "events": {
        "type": "belongsTo",
        "model": "event",
        "foreignKey": "eventId"
      },
      "users": {
        "type": "belongsTo",
        "model": "user",
        "foreignKey": "userId",
        "required": true
      }
    },
    "acls": [],
    "methods": {}
  }